{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": false,
      "module": "commonjs",
      "target": "ES5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "preserveConstEnums": true,
      "esModuleInterop": true,
      "baseUrl": "./src",
      "paths": {
        "styles/*": ["assets/styles/*"],
        "components/*": ["pages/components/*"],
        "store":["store/store"],
        "hooks":["store/hooks"]
      }
    },
    "include": [
      "./src/**/*"
    ]
    
  }